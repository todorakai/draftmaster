  <!-- Main Title -->
  <text x="450" y="280" font-family="Arial, sans-serif" font-size="72" font-weight="bold" 
        fill="white" letter-spacing="2">DRAFT MASTER</text>
  
  <!-- Subtitle -->
  <text x="450" y="340" font-family="Arial, sans-serif" font-size="32" font-weight="normal" 
        fill="white" opacity="0.9">AI-Powered League of Legends</text>
  <text x="450" y="385" font-family="Arial, sans-serif" font-size="32" font-weight="normal" 
        fill="white" opacity="0.9">Draft Assistant</text>
  
  <!-- Features -->
  <g transform="translate(450, 440)">
    <text y="0" font-family="Arial, sans-serif" font-size="24" fill="#60a5fa">
      ✓ AI Champion Recommendations
    </text>
    <text y="40" font-family="Arial, sans-serif" font-size="24" fill="#60a5fa">
      ✓ Team Synergy Analysis
    </text>
    <text y="80" font-family="Arial, sans-serif" font-size="24" fill="#60a5fa">
