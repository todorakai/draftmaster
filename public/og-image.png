    <path d="M -7 -42 L 7 -42 L 12 -15 L -12 -15 Z" 
          fill="white" opacity="0.95"/>
    <rect x="-37" y="-15" width="75" height="12" rx="3" 
          fill="white" opacity="0.95"/>
    <circle cx="-7" cy="-60" r="3" fill="#3b82f6"/>
    
    <!-- Strategic Lines -->
    <line x1="30" y1="-37" x2="52" y2="-52" stroke="white" stroke-width="3" opacity="0.7"/>
    <line x1="30" y1="-22" x2="52" y2="-22" stroke="white" stroke-width="3" opacity="0.7"/>
    <line x1="30" y1="-7" x2="52" y2="7" stroke="white" stroke-width="3" opacity="0.7"/>
    
    <circle cx="45" cy="-52" r="4" fill="white" opacity="0.8"/>
    <circle cx="57" cy="-22" r="4" fill="white" opacity="0.8"/>
    <circle cx="57" cy="7" r="4" fill="white" opacity="0.8"/>
