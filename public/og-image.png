    <line x1="600" y1="0" x2="600" y2="630" stroke="white" stroke-width="1"/>
    <line x1="900" y1="0" x2="900" y2="630" stroke="white" stroke-width="1"/>
  </g>
  
  <!-- Logo Circle -->
  <circle cx="250" cy="315" r="120" fill="url(#logoGradient)" opacity="0.9"/>
  
  <!-- Knight Icon -->
  <g transform="translate(250, 315)">
    <path d="M -30 -52 L -22 -67 L -7 -75 L 7 -72 L 15 -63 L 18 -52 L 15 -45 L 7 -42 L -7 -45 L -22 -48 Z" 
